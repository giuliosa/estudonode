<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Notícias</title>
		</head>
	<body>

		<h1>Notícias</h1>

		<table>
			<tr>
				<td>Data</td>
				<td>Título</td>
				<td>Notícia</td>
			</tr>
			<% for(var i = 0; i < noticias.length; i++){ %>
				<tr>
					<td><%= noticias[i].data %></td>
					<td><%= noticias[i].titulo %></td>
					<td><%= noticias[i].conteudo %></td>
				</tr>
			<% } %>
		</table>

	</body>
</html>
